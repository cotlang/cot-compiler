// Test while loop with 'and' condition
fn main() i64 {
    var i: i64 = 0
    var count: i64 = 0

    // This should print 0,1,2,3,4 and count should be 5
    while (i < 10 and i < 5) {
        println("i = " + string(i))
        i = i + 1
        count = count + 1
    }

    println("count = " + string(count))
    println("final i = " + string(i))

    if (count != 5) {
        println("ERROR: Expected count=5, got " + string(count))
        return 1
    }

    return 0
}
