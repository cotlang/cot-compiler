// Test 11: Enum and switch (expected: 1)
enum Color {
    Red,
    Green,
    Blue,
}

fn main() i64 {
    const c = Color.Green
    var result = -1
    switch (c) {
        Color.Red => { result = 0 }
        Color.Green => { result = 1 }
        Color.Blue => { result = 2 }
    }
    println(result)
    return result
}
